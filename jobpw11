server=158.160.27.62
docker run --rm -dv ./files:/usr/share/nginx/html -p 9889:80 nginx
curl -i http://$server:9889 | grep 200
curl -o downloaded.html http://$server:9889
test $(md5sum ./files/index.html ./downloaded.html | awk '{print $1}' | uniq | wc -l) == 1 
docker ps | grep docker | awk '{print $1}' | xargs docker stop
